    "metalsmith": "^2.3.0",
    "minimatch": "^3.0.0",
    "multimatch": "^2.1.0",
    "openurl": "1.1.1",
    "opn": "^4.0.2",
    "ora": "^0.4.0",
    "portscanner": "^2.1.1",
